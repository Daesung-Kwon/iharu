{
  "expo": {
    "name": "i하루",
    "slug": "daily-schedule-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "fonts": [
      "./assets/fonts/BMJUA_ttf.ttf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iharu.app",
      "requireFullScreen": false,
      "deviceFamily": [
        "ipad"
      ],
      "infoPlist": {
        "NSUserTrackingUsageDescription": "사용자에게 최적화된 광고를 제공하기 위해 기기 식별 정보를 사용합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.iharu.app",
      "tabletOnly": true,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FFF8E7",
          "sounds": []
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9978285135642181~1318901506",
          "iosAppId": "ca-app-pub-9978285135642181~7551710314"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "31a89f46-760d-491d-8ba6-a3ffda29ad8d"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "ds.kwon"
  }
}