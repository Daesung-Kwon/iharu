{
  "expo": {
    "name": "i하루",
    "slug": "daily-schedule-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8E7"
    },
    "fonts": [
      "./assets/fonts/BMJUA_ttf.ttf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dailyschedule.app",
      "requireFullScreen": false,
      "deviceFamily": ["ipad"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF8E7"
      },
      "package": "com.dailyschedule.app",
      "tabletOnly": true,
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FFF8E7",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}


